//Count ways to n'th stair

import java.io.*;
import java.util.*;

class GFG {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();

        while(t-- > 0) {
            int n = sc.nextInt();
            Solution obj = new Solution();
            System.out.println(obj.countWays(n));
        }
    }
}

class Solution {   
    int countWays(int n) {
        return (1 + (n / 2));
    }
}
